<script>

	$(document).ready(function() {
		$("#idfilech").on('change',function(){
		  $("#submission").click();
    	});
	});
	$('form').submit(function(e) {
		if(($(this).attr('id')).indexOf("formularyPoster2") != -1){
			 $(this).unbind('submit').submit()
		}else{
		 e.preventDefault();
		    $.ajax( {
		        url: "${root}flume/fileupload",
		        type: 'POST',
		        data: new FormData( $("#formularyPoster")[0] ),
		        processData: false,
		        contentType: false
		    }).done(function( data ) {
		    	if(data){
		    		//If data is only a processor, elements must match
		    		if($("#volatileMemory").children().length > 0){
		    			var jsnTr = JSON.parse(data);
						var jsnContent = jsnTr[0];
						var jxExternal;
						for (var prop in jsnContent) {
							if($("#"+prop).html()){
								var htmInn = JSON.parse($("#"+prop).html());
								var ajaxInn = jsnContent[prop];		
								if(ajaxInn["appname"]){
									jxExternal = ajaxInn["appname"];
									if(htmInn["appname"]){
										if(ajaxInn["appname"] == htmInn["appname"]){
											loadCanvas(data);
										}else{
											$('#resultWInfoIn').html("<b>Warning!</b>: There are some strange elements in canvas. It could cause misconfiguration.")
											$('#resultWInfo').fadeIn();
											$('#resultWInfo').fadeOut(5000);
										}
									}
								}	
							}
						}
						//if external name match with the one that if from volatile memory -> OK
						if($("#volatileMemory").text().indexOf("\"appname\":\""+jxExternal+"\"")){
							//TODO
						}
		    		}else{
		    			loadCanvas(data);	
		    		}
		    	}
		    });
		}
	  
	});
	
	
	function loadCanvas(data){
		var jsnTr = JSON.parse(data);
		var jsnContent = jsnTr[0];
		var jsnMetadat = jsnTr[1];
		
		for (var prop in jsnContent) {
			$("#volatileMemory").append('<div id="'+prop+'">'+JSON.stringify(jsnContent[prop])+"</div>");
		}
		
		for (var meta in jsnMetadat) {
			if(meta.indexOf("Source") != -1){
				var name = meta.replace("Source", " Source").replace("Bean","").replace(/\d+/g, '');
				if(jsnMetadat[meta]){
					mySources[meta.replace(/\d+/g, '')] = Number(meta.split("Bean")[1]);
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumesource' style=\"left: "+jsnMetadat[meta].left+"; top: "+jsnMetadat[meta].top+";\"><a href='#' onclick='showModal("+'"'+meta+'"'+");'>"+name+"</a></div>");
				}else{
					mySources[meta.replace(/\d+/g, '')] = Number(meta.split("Bean")[1]);
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumesource'><a href='#' onclick='showModal("+'"'+meta+'"'+");'>"+name+"</a></div>");
				}
				mySVG.drawLine({
					left_node:'.flumesource',
					right_node:'.flumechannel',
					horizantal_gap:10,
					error:true,
					width:1
				});
				$( ".flumesource" ).draggable({
					containment: '#enginacars',  
					drag: function(event, ui){mySVG.redrawLines();}
				});					
			}else if(meta.indexOf("Channel") != -1 && meta.indexOf("Sink") == -1){
				var name = meta.replace("Channel", " Channel").replace("Bean","").replace(/\d+/g, '');
				if(jsnMetadat[meta]){
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumechannel' style=\"left: "+jsnMetadat[meta].left+"; top: "+jsnMetadat[meta].top+";\"><a href='#' onclick='showModal("+'"'+meta+'"'+");'>"+name+"</a></div>");
					if(myChannels[meta.replace(/\d+/g, '')] < meta.split("Bean")[1] || typeof myChannels[meta.replace(/\d+/g, '')] === "undefined" ){
						myChannels[meta.replace(/\d+/g, '')] = Number(meta.split("Bean")[1]);
					}	
				}else{
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumechannel' ><a href='#' onclick='showModal("+'"'+meta+'"'+");'>"+name+"</a></div>");
					if(myChannels[meta.replace(/\d+/g, '')] < meta.split("Bean")[1] || typeof myChannels[meta.replace(/\d+/g, '')] === "undefined" ){
						myChannels[meta.replace(/\d+/g, '')] = Number(meta.split("Bean")[1]);
					}	
				}
				
				mySVG.drawLine({
					left_node:'.flumechannel',
					right_node:'.flumesource',
					horizantal_gap:10,
					error:true,
					width:1
				});

				$("#"+meta).draggable({
					containment: '#enginacars',  
					drag: function(event, ui){mySVG.redrawLines();}
				});
			}else if(meta.indexOf("Channel") != -1 && meta.indexOf("Sink") != -1){
				var sink = meta.split("from")[0];
				var channel = meta.split("from")[1];
				var tmpSink = " "+sink;
				var name = sink.replace("Sink", " Sink").replace("Bean","").replace(/\d+/g, '');
				if(jsnMetadat[meta]){
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumesink' style=\"left: "+jsnMetadat[meta].left+"; top: "+jsnMetadat[meta].top+";\"><a href='#' onclick='showModal("+'"'+sink+'"'+","+'"'+meta+'"'+");'>"+name+"</a></div>");
					
					if(mySinks[tmpSink.replace(/\d+/g, '')] < tmpSink.split("Bean")[1] || typeof mySinks[tmpSink.replace(/\d+/g, '')] === "undefined" ){
						mySinks[tmpSink.replace(/\d+/g, '')] = Number(tmpSink.split("Bean")[1]);
					}	
					
				}else{
					$("#innercanvas").prepend("<div id='"+meta+"' class='flumesink'><a href='#' onclick='showModal("+'"'+sink+'"'+","+'"'+meta+'"'+");'>"+name+"</a></div>");
					if(mySinks[tmpSink.replace(/\d+/g, '')] < tmpSink.split("Bean")[1] || typeof mySinks[tmpSink.replace(/\d+/g, '')] === "undefined" ){
						mySinks[tmpSink.replace(/\d+/g, '')] = Number(tmpSink.split("Bean")[1]);
					}	
					
				}
				mySVG.drawLine({
					left_node:'#'+channel,
					right_node:'#'+meta,
					horizantal_gap:10,
					error:true,
					width:1
				});
				
				mySVG.drawLine({
					left_node:'#'+meta,
					right_node:'#'+channel,
					horizantal_gap:10,
					error:true,
					width:1
				});

				$("#"+meta).draggable({
					containment: '#enginacars',  
					drag: function(event, ui){mySVG.redrawLines();}
				});
			}
		}
	}

	var mySources = {};
	var myChannels = {};
	var mySinks = {};
	var mySVG = $('#innercanvas').connect();
	
	var channelHTML = '<div class="col-sm-12">\
						<form class="form-inline">\
							<div class="form-group">\
								<label><b>Channel name:</b></label>&nbsp;&nbsp<input type="text" class="form-control input-sm" id="channelname">\
							</div>\
						<div class="col-sm-4 pull-right">\
							<div class="dropdown">\
								<button class="btn btn-primary dropdown-toggle pull-right" type="button" data-toggle="dropdown">Sinks<span class="caret"></span></button>\
							<ul id="dropDSik" class="dropdown-menu">\
						</div>\
					</div>';
	
	//Utils functions
	function getUrl(type) {
		var urlGo = "";
		if(type.indexOf("Source") != -1){
			urlGo = "${root}source/modal"
		} else if(type.indexOf("Channel") != -1){
			urlGo = "${root}channel/modal"
		} else if(type.indexOf("Sink") != -1){
			 urlGo = "${root}sink/modal"
		}
		return urlGo;
	}
	
	String.prototype.replaceAll = function(searchStr, replaceStr) {
	    var str = this;
	    searchStr = searchStr.replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&');	    
	    return str.replace(new RegExp(searchStr, 'gi'), replaceStr);
	};
	
	function getSource(type){
		var name = type.replace("Source", " Source").replace("Bean","");
		
		if($(".flumesource")[0]){
			var tmp = mySources[type];
			mySources[type] = tmp+1;
			$('#resultWInfoIn').html("<b>Warning!</b>: We currently accept only one source each time. It does not mean that multiple collectors from the same source can  be configured.")
			$('#resultWInfo').fadeIn();
			$('#resultWInfo').fadeOut(15000);
		}else{
			mySources[type] = 1;
			$("#innercanvas").prepend("<div id='"+type+mySources[type]+"' class='flumesource'><center><a href='#' onclick='showModal("+'"'+type+mySources[type]+'"'+");return false;'>"+name+"</a></center></div>");
			$( ".flumesource" ).draggable({containment: '#enginacars'});
			mySVG.drawLine({
				left_node:'.flumesource',
				right_node:'.flumechannel',
				horizantal_gap:10,
				error:true,
				width:1
			});
			$( ".flumesource" ).draggable({
				containment: '#enginacars',  
				drag: function(event, ui){mySVG.redrawLines();}
			});
		}
	}
	
	function getChannel(channel, source){
		var name = channel.replace("Channel"," Channel");
		if(myChannels[channel]){
			var tmp = myChannels[channel];
			myChannels[channel] = tmp+1;
		}else{
			myChannels[channel] = 1;
		}
		$("#innercanvas").prepend("<div id='"+(channel+myChannels[channel]).trim()+"' class='flumechannel'><center><a href='#' onclick='showModal("+'"'+channel.trim()+myChannels[channel]+'"'+");return false;'>"+name+"</a></center></div>");
		mySVG.drawLine({
			left_node:'#'+source,
			right_node:'#'+(channel+myChannels[channel]).trim(),
			horizantal_gap:10,
			error:true,
			width:1
		});

		$("#"+(channel+myChannels[channel]).trim()).draggable({
			containment: '#enginacars',  
			drag: function(event, ui){mySVG.redrawLines();}
		});
		
		$('#resultCInfo').html("<b>Success!:</b> New <b>"+name+"</b> created on canvas area.");
		$('#resultCInfoPop').fadeIn();
		$('#resultCInfoPop').fadeOut(2000);

	}
	
	function getSink(type, channelFrom){
		   var name = type.replace("Sink", " Sink");
			if(mySinks[type]){
				var tmp = mySinks[type];
				mySinks[type] = tmp+1;
			}else{
				mySinks[type] = 1;
			}
			$("#innercanvas").prepend("<div id='"+(type+mySinks[type]).trim()+"from"+channelFrom+"' class='flumesink'><b><a href='#' style='color: #000000;' onclick='showModal("+'"'+type+mySinks[type]+'"'+","+'"'+(type+mySinks[type]).trim()+"from"+channelFrom+'"'+");return false;'><center>"+name+"</center></a></b></div>");
			mySVG.drawLine({
				left_node:'#'+channelFrom,
				right_node:'#'+((type+mySinks[type]).trim()+"from"+channelFrom).trim(),
				horizantal_gap:10,
				error:true,
				width:1
			});

			$( "#"+((type+mySinks[type]).trim()+"from"+channelFrom).trim() ).draggable({
				containment: '#enginacars',  
				drag: function(event, ui){mySVG.redrawLines();}
			});
			
			$('#resultCInfo').html("<b>Success!:</b> New <b>"+name+"</b> created on canvas area.");
			$('#resultCInfoPop').fadeIn();
			$('#resultCInfoPop').fadeOut(2000);
			
	}
	
	function capitalizeFirstLetter(string) {
	    return string.charAt(0).toUpperCase() + string.slice(1);
	}
	
	function isEmpty(str) {
	    return (!str || 0 === str.length);
	}
	
	function addMetaType(type){
			$("#"+type+"Group").append('<label class="col-sm-2 col-form-label"><b>'+capitalizeFirstLetter(type)+':</b></label>\
					<input type="text" class="form-control" placeholder="" aria-label="'+capitalizeFirstLetter(type)+'" aria-describedby="basic-addon2"> \
				    <div class="input-group-append"> \
				   		 <button class="btn btn-outline-secondary" onclick="addMetaType('+"'"+type+"'"+');return false;" type="button">+</button> \
				  	</div> \
				</div>');
	}
	
	/////
	
	function showModal(type, chnalCondition){
		var name = type.replace("Source", " Source").replace("Channel", " Channel").replace("Sink", " Sink").replace("Bean","").replace(/[0-9]/g, "");
		if($("#"+type.trim()+"saved").length || $("#"+chnalCondition+"saved").length){
			if(type.indexOf("Source") != -1){
				varresponseJson = JSON.parse($("#"+type.trim()+"saved").text());
				printModalContent(responseJson["stored"],name,type,"","");
				$('#applicativename').val(responseJson["appname"]);
				//$('#numbOfSource').val(responseJson["numbOfSource"]);
			}else if(type.indexOf("Channel") != -1){
				varresponseJson = JSON.parse($("#"+type.trim()+"saved").text());
				printModalContent(responseJson["stored"],name,type,"");
				$('#channelname').val(responseJson["channelname"]);
			}else if(type.indexOf("Sink") != -1){
				varresponseJson = JSON.parse($("#"+chnalCondition+"saved").text());
				printModalContent(responseJson["stored"],name,type,chnalCondition);
			}
		}else{
			$.ajax({
				cache:false,
				type:"post",
				url:getUrl(type),
				data: "source="+type.replace(/[0-9]/g, "").trim(),
				success:function(response) {
				varresponseJson = JSON.parse(response);
				if(!$.isEmptyObject(responseJson)) {
				printModalContent(responseJson,name,type,chnalCondition);
				}
				}
			});		
		}
		$('#myModal').modal('show'); 
	}
	
	function saveContent(idresource){
		var formLabel = [];
		var formData = [];
		var jsonContent = {};
		var jsonContentSaved = {};
		var jsonInterceptor = [];
		var jsonSelector = [];
		var jsonConverter = [];
		var jsonSerializer = [];
		var i=0; 
		var j=0;
		var flag = true;
		//Common part
		$('#formInfo').find('label').each(function() {
				formLabel[i] = this.id;
				i++;
		});
		$('#serializerGroup').find('input').each(function() {
			jsonSerializer.push(this.value);
		});
		$('#converterGroup').find('input').each(function() {
			jsonConverter.push(this.value);
		});
		$('#interceptorGroup').find('input').each(function() {
			jsonInterceptor.push(this.value);
		});
		$('#selectorGroup').find('input').each(function() {
			jsonSelector.push(this.value);
		});
		$('#formInfo').find('input').each(function() {
			formData[j] = this.value;
			j++;
		});	
		for(var k = 0; k < formData.length; k++){
			if(!isEmpty(formLabel[k])){
				jsonContent[formLabel[k]]=formData[k];
			}
		}
		jsonContent["interceptors"] = jsonInterceptor;
		jsonContent["selectors"] = jsonSelector;
		jsonContent["converters"] = jsonConverter;
		jsonContent["serializers"] = jsonSerializer;
		if(idresource.indexOf("Source") !=-1) {
			jsonContentSaved["appname"]=$('#applicativename').val();
			//jsonContentSaved["numbOfSource"]=$('#numbOfSource').val();
			jsonContentSaved["stored"]=jsonContent;
			if($('#'+idresource+'saved').length){
				$('#'+idresource+'saved').remove();
				$('#volatileMemory').append("<div id='"+idresource+'saved'+"' >"+JSON.stringify(jsonContentSaved)+"</div>");
				
			}else{
				$('#volatileMemory').append("<div id='"+idresource+'saved'+"' >"+JSON.stringify(jsonContentSaved)+"</div>")
			}
		}else if(idresource.indexOf("Channel") !=-1 && idresource.indexOf("Sink") ==-1){
			if($('#channelname').val() === ""){
				$('#contentAdv').html("<b>Warning!</b>: Channel name can not be empty.")
				$('#contentAdv').fadeIn();
				$('#contentAdv').fadeOut(15000);
			}else{
				$('#'+idresource+'saved').remove();
					if($('#channelname')){
					   jsonContentSaved["channelname"]=$('#channelname').val();	
					}else{
						var channName = idresource.split("from")[1].replace("saved","");
						jsonContentSaved["channelname"]=$('#'+channName).val();
					}
					jsonContentSaved["stored"]=jsonContent;
					$('#volatileMemory').append("<div id='"+idresource+'saved'+"' >"+JSON.stringify(jsonContentSaved)+"</div>");
					$('#myModal').modal('hide');	
				
		    }
		}else if(idresource.indexOf("Channel") !=-1 && idresource.indexOf("Sink") !=-1){
				$('#'+idresource+'saved').remove();
